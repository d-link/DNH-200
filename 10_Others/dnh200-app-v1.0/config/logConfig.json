{
  "appenders": {
    "console": {
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%[[%d{yyyy-MM-dd hh:mm:ss.SSS}] [%p]%] %m"
      }
    },
    "media": {
      "type": "dateFile",
      "filename": "/mnt/sdcard/dnhlog/MS.log",
      "pattern": ".yyyy-MM-dd",
      "compress": true,
      "keepFileExt": true,
      "alwaysIncludePattern": true,
      "daysToKeep": 7,
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}] [%p] %m"
      }
    },
    "core": {
      "type": "dateFile",
      "filename": "/mnt/sdcard/dnhlog/CS.log",
      "pattern": ".yyyy-MM-dd",
      "compress": true,
      "keepFileExt": true,
      "alwaysIncludePattern": true,
      "daysToKeep": 7,
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}] [%p] %m"
      }
    },
    "web": {
      "type": "dateFile",
      "filename": "/mnt/sdcard/dnhlog/WS.log",
      "pattern": ".yyyy-MM-dd",
      "compress": true,
      "keepFileExt": true,
      "alwaysIncludePattern": true,
      "daysToKeep": 7,
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}] [%p] %m"
      }
    }
  },
  "categories": {
    "default": {"appenders": ["console"], "level": "debug"},
    "media": {"appenders": ["console", "media"], "level": "debug"},
    "core": {"appenders": ["console", "core"], "level": "debug"},
    "web": {"appenders": ["console", "web"], "level": "debug"}
  }
}