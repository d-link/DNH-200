  {
    "bind": "WirelessSchedule",
    "version": [
      {
        "config": [
          {
            "bind": "dot11ScheduleStatus",
            "name": "Wireless Schedule",
            "type": "DAYSSELECT",
            "enums": [
              {
                "text": "Enable",
                "value": 1
              },
              {
                "text": "Disable",
                "value": 2
              }
            ]
          },
          {
            "bind": "dot11ScheduleRules",
            "name": "Schedule Rules",
            "type": "TABLE",
            "entrys": [
              {
                "name": "Index",
                "bind": "dot11ScheduleSSIDIndex",
                "type": "SELECT",
                "enums": [
                  {
                    "text": "Primary SSID 2.4G",
                    "value": 1
                  },
                  {
                    "text": "SSID 1 2.4G",
                    "value": 2
                  },
                  {
                    "text": "SSID 2 2.4G",
                    "value": 3
                  },
                  {
                    "text": "SSID 3 2.4G",
                    "value": 4
                  }
                ]
              },
              {
                "name": "Name",
                "bind": "dot11ScheduleRuleName",
                "type": "TEXT",
                "placeholder": "(between 1 and 32 characters)"
              },
              {
                "name": "Day(s)",
                "bind": "dot11ScheduleDaysSelect",
                "type": "DAYSSELECT",
                "placeholder": "a directive will replace it"
              },
              {
                "name": "All Day(s)",
                "bind": "dot11ScheduleAllDaySelect",
                "type": "SELECT",
                "enums": [
                  {
                    "text": "Enable",
                    "value": 1
                  },
                  {
                    "text": "Disable",
                    "value": 2
                  }
                ]
              },
              {
                "name": "Start Time",
                "bind": "dot11ScheduleRuleStartTime",
                "type": "TIME",
                "show": {
                  "bind": "dot11ScheduleAllDaySelect",
                  "valueIndex": 1
                },
                "placeholder": "(hour:minute,24 hour time)"
              },
              {
                "name": "End Time",
                "bind": "dot11ScheduleRuleEndTime",
                "type": "TIME",
                "show": {
                  "bind": "dot11ScheduleAllDaySelect",
                  "valueIndex": 1
                },
                "placeholder": "(hour:minute,24 hour time)"
              }
            ]
          }
        ]
      }
