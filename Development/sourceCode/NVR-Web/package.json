{
  "name": "nuclias-web",
  "version": "1.00.23",
  "private": true,
  "bin": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=dev node ./bin/www",
    "pkg": "pkg -d -t node8-linux .",
    "pull-pm2": "docker pull keymetrics/pm2:8-alpine",
    "build": "docker build -t dlink/nuclias-web .",
    "postbuild": "docker save dlink/nuclias-web > web.tar"
  },
  "engines": {
    "node": ">=5.2.0"
  },
  "pkg": {
    "scripts": [
      "routes/*.js",
      "lib/*.js",
      "lib/process/*.js",
      "cwmcontroller/*.js",
      "node_modules/unicode-properties/*.js",
      "node_modules/fontkit/*.js",
      "node_modules/dnh-common/index.js",
      "node_modules/dnh-common/common/*.js",
      "node_modules/dnh-common/db/**/*.js",
      "node_modules/dnh-common/doc/*.js",
      "node_modules/dnh-common/log/*.js"
    ],
    "assets": [
      "public/**/*.js",
      "public/images/*",
      "public/website1/*.html",
      "public/website1/*.ico",
      "public/website1/public/fonts",
      "public/website1/public/fonts/simsun.ttf",
      "public/website1/public/images",
      "public/website1/public/styles",
      "public/website1/public/vendor/angular-block-ui/dist/angular-block-ui.min.css",
      "public/website1/public/vendor/angular-ui-grid/ui-grid.min.css",
      "public/website1/public/vendor/angular-gridster/angular-gridster.min.css",
      "public/website1/public/vendor/angular-ui-select/select.min.css",
      "public/website1/public/vendor/angular-ui-grid/fonts/ui-grid.ttf",
      "public/website1/public/vendor/angular-ui-grid/fonts/ui-grid.woff",
      "public/website1/public/vendor/**/*.map",
      "public/website1/public/vendor/underscore/underscore-min.map",
      "public/website1/public/vendor/**/*.wof",
      "public/website1/public/users/*.html",
      "public/website1/nuclias_connect/*.html",
      "public/website1/nuclias_connect/*.ico",
      "public/website1/nuclias_connect/assets",
      "public/website1/nuclias_connect/images",
      "public/website1/nuclias_connect/scripts/**/*.css",
      "public/website1/nuclias_connect/scripts/**/*.html",
      "public/website1/nuclias_connect/views/**/*.html",
      "public/website1/appSite/*.html",
      "public/website1/appSite/*.ico",
      "public/website1/appSite/assets",
      "public/website1/appSite/fonts",
      "public/website1/appSite/images",
      "public/website1/appSite/scripts/**/*.css",
      "public/website1/appSite/scripts/**/*.html",
      "public/website1/appSite/views/**/*.html",
      "node_modules/**/*.trie",
      "node_modules/dnh-common/db/doc/**/*",
      "node_modules/pdfkit/js/font/data/*.afm"
    ]
  },
  "dependencies": {
    "async": "^3.1.0",
    "blob-stream": "^0.1.3",
    "body-parser": "~1.13.2",
    "captchapng": "0.0.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cron": "^1.7.1",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.2.0",
    "dnh-common": "git+https://testdlink:dlinktest123@github.com/d-link/DView7-SP1-Common#dnh100",
    "ejs": "^2.7.1",
    "expose-gc": "^1.0.0",
    "express": "^4.16.4",
    "iconv-lite": "^0.4.15",
    "ipp": "^2.0.0",
    "json-beautify": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "line-reader": "^0.4.0",
    "lodash": "^3.10.1",
    "microtime": "^2.1.8",
    "moment": "^2.24.0",
    "morgan": "^1.6.1",
    "multiparty": "^4.1.2",
    "node-forge": "^0.7.6",
    "node-gyp": "^3.8.0",
    "paypal-rest-sdk": "^1.8.1",
    "pdfkit": "^0.8.3",
    "pnglib": "0.0.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "should": "^9.0.2",
    "sshpk": "^1.16.1",
    "supertest": "^1.2.0",
    "tar-fs": "^2.0.0",
    "underscore": "^1.8.3",
    "unzip": "^0.1.11",
    "uuid": "^3.3.2",
    "validator": "^11.1.0",
    "xlsx": "^0.15.1"
  }
}
